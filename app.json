{
  "expo": {
    "name": "RidesData",
    "slug": "Rides-Data",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/roninimage.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "bikerides.data",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/roninimage1.jpg",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/raider.jpeg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/avatar.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "eecc79e0-80b6-4a07-b7c2-f3b954519c85"
      }
    }
  }
}
